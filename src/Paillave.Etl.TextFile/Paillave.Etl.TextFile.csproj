<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>Paillave.EtlNet.TextFile</PackageId>
    <Version>2.1.17-beta</Version>
    <Authors>St√©phane Royer</Authors>
    <Company></Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://paillave.github.io/Etl.Net/</PackageProjectUrl>
    <PackageTags>ETL .net core SSIS reactive text file csv</PackageTags>
    <Product>ETL.net text files extensions</Product>
    <PackageIcon>NugetIcon.png</PackageIcon>
    <Description>Extensions for Etl.Net to read and write csv and fixed column width files</Description>
    <LangVersion>latest</LangVersion>
